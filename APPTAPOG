                 SEGMENT(TROOTSEG) INTO(IOAREAXX)
                 WHERE(TSRARLOG = ARLOG_K);

            KFIOROOT = ADDR(IOAREAXX);
            TFIOROOT = KFIOROOT;
         /*---------------------------------------------------------*/
         /*                 ANYpAPKTOr AP.kOcAPIArMOY               */
         /*---------------------------------------------------------*/

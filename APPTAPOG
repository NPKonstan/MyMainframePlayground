                          INTO    (IOAREA)
                          LENGTH  (RLENGTH)
                          RIDFLD  (EFKEY1)
                          UPDATE;
         SAVEACCN = EFNUMB1;          /* uYkAnHr APIhMOY EccPAuHr    */

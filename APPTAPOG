            FLG1 = 0; /* FLAG GIA THN EKTYPOSH */
            IF REF2B = '0'B ! EFREF2 = ' ' THEN DO;
               EFREF2 = WWONOMA;
               FLG1   = 1;
            END;
         END;
         EFDIAS   = ' ';
         IF SHMAIA5 = 5 THEN DO;

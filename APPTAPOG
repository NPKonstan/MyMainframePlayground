            EFNOTE2  = WLOG8;    /* ARLOG GIA ENSHMA    */
            EFAMOU2  = WPOS10;    /* POSO XORIS ELEGXO   */
         END;
                                 /* REF1 = 20101        */
                                 /* REF2 = AM YYYYMM    */
         IF SHMAIA5 = 5 THEN DO;
            EFKATMA2 = CUASKAT;
            EFAFM2   = FAFM9;    /* AFM GIA ENSHMA 9 CHFIA */
            EFNOTE2  = WLOG8;    /* ARLOG               */
            EFAMOU2  = WWAFM;   /* AFM EMPORIKHS       */
         END;

         ZILKAT  ='1111'B;
         ZILAFM  ='1111'B;
         ZILNOT  ='1111'B;
         ZILAMO  ='1111'B;
         YY2      = YY1;
         DD2      = DD1;
         MM2      = MM1;

         YY42     = YY1;
         DD4      = DD1;
         MM4      = MM1;
         IF YY1 > 80 THEN DO;
            YY41 = 19;
            YY42 = YY1;
         END;
         IF YY1 < 80 THEN DO;
            YY41 = 20;
            YY42 = YY1;
         END;
         YY42    = YY1;
         EFRDATE2 = DATE4;     /* YYYYMMDD               */
         EFRTIME  = EIBTIME/100;
         IF SHMAIA = 1 THEN DO;
            EFDATID  = 01;        /* uOPOI KEpYO            */
         END;
         IF SHMAIA = 2 THEN DO;
            EFDATID  = 02;        /* KATAGRAFH LOGARIASMON  */
         END;
         IF SHMAIA = 3 THEN DO;
            EFDATID  = 03;        /* FPA                    */
         END;
         IF SHMAIA = 4 THEN DO;
            EFDATID  = 04;        /* ENSHMA                 */
         END;
         IF SHMAIA5 = 5 THEN DO;
            EFDATID  = 02;        /* APOGRAFH  DPS          */
            EFCLAMOU = 0;              /* DEN YPARXEI POSO       */

         EFTYPE2  = 2;         /* RECORD PLHRVMHS        */
         EFSTATUS = 0;         /* SE ISXY                */
         EFBRANC2 = CUASKAT;   /* TRAPEZA KATMA PLHRVMHS */
         IF SHMAIA = 1 THEN DO;

      /*IF EFWREF1 = 0405 & DATESYS > 110331 THEN DO;*/
      /*IF EFWREF1 = 0405 & DATESYS > 110513 THEN DO;   30.3.11 1159*/
      /*IF EFWREF1 = 0405 & DATESYS > 110531 THEN DO;*/ /*16.5.11 1694*/

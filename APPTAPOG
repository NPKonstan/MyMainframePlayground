         /* PUT CONTROL RECORD ME NEO A" dIAhErIMO APIhMO EccPAuHr */
         /*--------------------------------------------------------*/
         EXEC CICS REWRITE DATASET ('EFORIA1')
                   FROM(IOAREA);

         /*-TELOS ENHMERVSEVN TOY ARXEIOY EFORIA1 VS PROS FORO ----*/


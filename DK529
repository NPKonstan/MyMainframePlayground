  /**************************************************************
  *  WRITE_ACC              : PROC;                             *
  *                                                             *
  **************************************************************/
     WRITE_ACC              : PROC;
       IF WS_IS_TAM = 1 THEN DO;
              WRITE FILE(ACFILET ) FROM(ACCREC);
              CNT_REC_WRITTEN_TAM = CNT_REC_WRITTEN_TAM + 1;
       END;
       ELSE DO;
              WRITE FILE(ACCFILE ) FROM(ACCREC);
              CNT_REC_WRITTEN_KAT = CNT_REC_WRITTEN_KAT + 1;
       END;


       ACCREC = ''; /* CLEAR RECORD  */

     END  WRITE_ACC             ;

  /**************************************************************
  *  WRITE_ACC                   END                            *
  **************************************************************/

 /*                                                                  */
  /**************************************************************
  *  PRINT_FOR_TEST                                             *
  *                                                             *
  **************************************************************/
     PRINT_FOR_TEST        : PROC;

      REC_COUNTER = REC_COUNTER +1 ;

      IF WS_IS_TAM = 1 & ONLY_ONCE = 0 THEN DO ;/*EINAI TAMIEYTHRIO*/
         REC_COUNTER =0;
         ONLY_ONCE = 1;
      END;

      IF REC_COUNTER < MAX_REC_COUNTER THEN DO ;
         ERLIN2 =
            ' LOGAR   ' !!  ACCNUMBER    !!
            ' BALANCE ' !!  ACCBALANCE   !!
            ' йатг  4 ' !!  ACC4KATH     !!
            ' KATH 10 ' !!  ACC10KATH    ;
        WRITE FILE(PRT) FROM(ERLINE);
         /*
         ERLIN2 =
            ' CUSNAME ' !!  ACCCUSNAME   !!
            ' CIDBNUM ' !!  ACCCIDBNUM   !!
            ' EPAGELM ' !!  ACCEPAG      !!
            ' BRANCH  ' !!  ACCBRANCH    ;

        WRITE FILE(PRT) FROM(ERLINE);
        */
         ERLIN2 =
            ' йатоийос ' !!  ACCKATOIK    !!
            ' выяа  ' !!  ACCCOUNTRY ;

        WRITE FILE(PRT) FROM(ERLINE);
     END;
     END  PRINT_FOR_TEST ;

  /**************************************************************
  *  WRITE_ACC                   END                            *
  **************************************************************/

  TELOS:

  /***********************************************/
  /**   PRINT TOTALS     *************************/
  /***********************************************/
   ERLIN1    = '1';  /*SKIP LINE*/

   ERLIN2 = '   ейтупысг сумокым тоу пяоцяаллатос *DK529*   ';
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN1    = ' ';
   ERLIN2 = ' глеяолгмиа KAI ыяа яогс :  ' !!DATETIME ;
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = 'сумоко еццяажым поу упаявоум сто тал20                :'!!
   CNT_REC_READ                  ;
   WRITE FILE(PRT) FROM(ERLINE);
   ERLIN2 = 'сумокийо посом йатахесеым сто    тал20                :'!!
               TOT_KAT ;
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = 'сумокийо посом талиеутгяиоу сто  тал20                :'!!
               TOT_TAM ;
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = 'сумоко еццяажым поу епенеяцасхгйам                    :'!!
   CNT_REC_PROCCED               ;
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = 'сумоко еццяажым поу дем упаяв.  сто T04CNV97          :'!!
   CNT_REC_REJECT_T04CNV97       ;

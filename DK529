     /***********************************************************/
 DK529 : PROC OPTIONS (MAIN);
 DCL DK4TO6B EXTERNAL ENTRY(FIXED(5,0),    /*йахокийо*/
                            CHAR(2),     /*INPUT йыдийос епацйеклатос*/
                            CHAR(1),     /*INPUT йыдийос жусийо-молийо*/
                            FIXED(9,0),  /*INPUT аяихлос коцаяиаслоу*/
                            FIXED(3,0)   /*OUTPUT еRROR FLAG*/
                            ) RETURNS(FIXED(7));
 FETCH DK4TO6B;

 DCL  TAM20      FILE RECORD INPUT ENV(
                 FB RECSIZE(90)                          );

 % INCLUDE  BKT20T ;

 DCL ACCFILE    FILE RECORD OUTPUT                                      00180002
                          ENV(FB RECSIZE(86) );                         00190002
 DCL ACFILET   FILE RECORD OUTPUT                                       00180002
                          ENV(FB RECSIZE(86) );                         00190002
 /*NEW FORMAT FOR ACCREC 24.01.2004 */

 DCL 1 ACCREC ,
   2 ACCNUMBER  CHAR(8)        ,/*  APихлос коцаяиаслоу               */
   2 ACCCUSNAME CHAR(30)       ,/*  пкгяес омолатепымуло пекатг       */
   2 ACCCIDBNUM CHAR(08)       ,/*  CIDB NUMBER                       */
   2 ACCBRANCH  CHAR(4)        ,/*  COD-BRANCH                        */
   2 ACC_F_N    CHAR(1)        ,/*  жусийо=1 молийо=2                 */
   2 ACCEPAG    CHAR(2)        ,/*  COD-епаццеклатос                  */
   2 ACCBALANCE PIC'S(12)9V,99',/*  диахесило                         */
   2 ACC4KATH   CHAR(4)        ,/*  йахокийо 4-ьгжио                  */
   2 ACC10KATH  CHAR(10)       ,/*  йахокийо 10ЬГЖИО    MSA           */
   2 ACCKATOIK  CHAR(1)        ,/*  KATOIKOS лг йатоийос              */
   2 ACCCOUNTRY CHAR(2)       ; /*  йыдийос выяас йатоийиас           */



     /* DECLARATIONS                         */

     DCL  EOF          FIXED(1)       INIT(0)  ;

     DCL  FLAG         FIXED(1)       INIT(0)  ;

     DCL  WS_CUSTOMER_FOUND FIXED(1)  INIT(0)  ;

     DCL  WS_IS_TAM    FIXED(1)  INIT(0)  ;
     DCL ERROR_FLG     FIXED DEC (3,0);


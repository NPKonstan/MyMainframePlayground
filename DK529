   ERLIN2 = 'сумоко еццяажым поу цяажтгйам сто аявеио талиеутгяиоу :'!!
   CNT_REC_WRITTEN_TAM           ;
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = 'сумоко амупаяйтым йахокийым = '!!C;
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = 'сумоко POSON ANYP йахокийым = '!!D;
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = '*******************************************************';
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = '* сумока посым,коцаяиаслым ама 6-ьгжио йахокийо *******';
   WRITE FILE(PRT) FROM(ERLINE);

   ERLIN2 = '*******************************************************';
   WRITE FILE(PRT) FROM(ERLINE);

  ERLIN2 ='йахокийо '!!506001!!' '!!B506001!!' '!!A506001     ;
   WRITE FILE(PRT) FROM(ERLINE);
  ERLIN2 ='йахокийо '!!506002!!' '!!B506002!!' '!!A506002     ;

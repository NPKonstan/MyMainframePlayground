         '515099 /           -*** ME kAhOr KxdIKO EpAccEkMATOr ***  ',
         '530001 / 5100000007-TAMIEYTHPIOY                          ',
         '530099 /           -*** ME kAhOr KxdIKO EpAccEkMATOr ***  ',
         '000000 /                                                  ',
         '000000 /                                                  ',
         '000000 /                                                  ',
         '000000 /                                                  '); ;

          /* 05.04.2007
 DCL LAT      CHAR(25) INIT ('ABcdEZHhIKkMNnOpPrTYuXwx␀');
 DCL GRE      CHAR(25) INIT ('абцдефгхийклмнопястужвьы ');     */

 DCL LAT      CHAR(28) INIT ('ABcdEZHhIKkMNnOpPrTYuXwx␀	▌');
 DCL GRE      CHAR(28) INIT ('абцдефгхийклмнопястужвьы    ');

 DCL (ONFILE,ONCODE,ONKEY,PRIORITY,TIME) BUILTIN;
 DCL (DATETIME,SUBSTR,NULL,ADDR,UNSPEC,TRANSLATE) BUILTIN;

  /*******************************************************************/
  /***********    M A I N   P R O G R A M    *************************/
  /*******************************************************************/

 %PAGE;
 ARXH :;

   CNT_REC_READ              = 0 ;
   CNT_REC_REJECT_CYPR       = 0 ;
   CNT_REC_PROCCED           = 0 ;
   CNT_REC_REJECT_T04CNV97   = 0 ;
   CNT_REC_REJECT_T04DOM01   = 0 ;
   CNT_REC_PROCCED_RIGHT     = 0 ;
   CNT_REC_WRITTEN_TAM       = 0 ;
   CNT_REC_WRITTEN_KAT       = 0 ;
   CNT_REC_FISICAL           = 0 ;
   CNT_REC_BIG_COMPANY       = 0 ;
   CNT_REC_SMALL_COMPANY     = 0 ;

   OPEN FILE(TAM20);

   OPEN FILE(ACCFILE);
   OPEN FILE(ACFILET);

   ON ENDFILE(TAM20)
        BEGIN ;
          GO TO  TELOS;
        END;

 /* * * * * * * * * * * * * * * * * * * * * * * */
 READ_FILE:;

     WS_IS_TAM = 0;
     READ  FILE(TAM20) INTO(T20REC);


     /*лгм епенеяцастгс то талиеутгяио*/
     IF T20ARLOGTAM < 80000000 THEN  DO;
        WS_IS_TAM = 1;
     END;
     ELSE DO;
        WS_IS_TAM = 2;
     END;
     /*лгм епенеяцастгс тA AKYROMENA  */
     IF T20AKYR    ^= '0'      THEN GO TO READ_FILE;

     CNT_REC_READ  = CNT_REC_READ  + 1;

     /*лгм епенеяцасхEIс тяапефес аккес ейтос елпояийгс*/
     /*оуте йатастглата  CYPRUS*/
     IF  (T20TRAP ^= 0 ) !
         (T20KATMA > 452 & T20KATMA < 460) THEN
          CNT_REC_REJECT_CYPR   = CNT_REC_REJECT_CYPR +1;
     ELSE DO ;                    /*                              */

    CNT_REC_PROCCED = CNT_REC_PROCCED   + 1;



    CALL FILL_TRIVIAL          ; /* ломо та еуйока  еды          */
    CALL LOAD_TBL              ; /* жоятысе пимайа апотекеслатым */
    CALL FILL_KATHOLIKO_10     ; /* то дейаьгжио йахокийо        */
    CALL FILL_CIDB_NUMBER      ; /* аяихлос   CIDB               */

    IF IS_TEST = 'YES' THEN
           CALL PRINT_FOR_TEST ;
    ELSE
           CALL WRITE_ACC        ; /* еццяажг сто аявеио ACC       */
   END; /*TELOS тоу оуте йупяос оуте аккг тяапефа*/

   GO TO READ_FILE;   /*READ AGAIN*/
  /**************************************************************
  *  FILL_TRIVIAL             START                             *
  *                                                             *
  **************************************************************/
     FILL_TRIVIAL           : PROC;

     ACCREC = ''; /* CLEAR RECORD FOR WRITING */

     WS_PIC9_ARACC    = T20ARLOGTAM ;

     ACCNUMBER        = WS_PIC9_ARACC  ;
     ACCCUSNAME  = TRANSLATE(T20ONOMA,GRE,LAT);

     /*ACCCUSNAME       = T20ONOMA    ; */

     ACCBALANCE       = T20POSON    ;
     IF WS_IS_TAM = 1 THEN
          TOT_TAM          = TOT_TAM   + T20POSON;
     ELSE
          TOT_KAT          = TOT_KAT   + T20POSON;

     WS_PIC9_ARKATMA  = T20KATMA      ;
     WS_ARKATMA       =  WS_PIC9_ARKATMA    ;
     ACCBRANCH        = '0'!!WS_ARKATMA;   /*йыдийос йатастглатос 4ьгж*/

     ACCEPAG          = T20EPAGELMA;       /* йыдийос епаццеклатос */

     WS_PIC9_KATH      = T20KATHO         ;

     ACC4KATH         =  WS_PIC9_KATHOL  ; /*тетяаьгжио йахокийо  */

     /*аккацг тгс йкгсгс тгс яоутимас,екецвоуле йаи циа кахос*/
     ERROR_FLG   = 0;
     WS_PIC9_KATH_B1   =  0 ;

     WS_PIC9_KATH_B1   =  DK4TO6B(T20KATHO ,
                                 T20EPAGELMA ,
                                 T20FN,
                                 T20ARLOGTAM ,
                                 ERROR_FLG
                                 ) ;
       /**********************************************************/
       /*Aккацг йахокийым,акка циа коцистийа акка,циа ейтойислоус*/
       /* апо 506086         се 506005                           */
       /* апо 506008         се 506002                           */
       /* апо 506009         се 506002                           */
       /* апо 511430         се 511401                           */
       /* Nпй 27-12-2004                                         */
       /* ------------------------------------------------------ */
       /* апо 511402         се 511401                           */
       /* Nпй 22-02-2005                                         */
       /**********************************************************/
         SELECT(WS_PIC9_KATH_B1);/*лгпыс вяеиафетаи аккацг йахокийоу ;*/

               WHEN(506086 )  DO;
                                  WS_PIC9_KATH_B = 506005  ; END;
               WHEN(506008 )  DO;
                                  WS_PIC9_KATH_B = 506002  ; END;
               WHEN(506009 )  DO;
                                  WS_PIC9_KATH_B = 506002  ; END;
               WHEN(511430 )  DO;
                                  WS_PIC9_KATH_B = 511401  ; END;
               WHEN(511402 )  DO;
                                  WS_PIC9_KATH_B = 511401  ; END;
               OTHERWISE      DO; /*дем вяеиафетаи аккацг йахокийоу*/
                                  WS_PIC9_KATH_B =  WS_PIC9_KATH_B1;
                              END;
         END; /*SELECT END*/

     IF T20FN = 'N' THEN
           ACC_F_N  = '2';  /* NOMIKO */
     ELSE
           ACC_F_N  = '1';  /* жусийо */

       ACCCOUNTRY  = T20CNTRY   ; /*  йыдийос выяас йатоийиас  */

       ACCKATOIK   = T20IDSTA   ; /*  KATOIKOS лг йатоийос     */
     END  FILL_TRIVIAL          ;
  /**************************************************************
  *  FILL_TRIVIAL                    END                        *
  **************************************************************/

  /**************************************************************
  *  FILL_KATHOLIKO_10      START                               *
  *                                                             *
  **************************************************************/
     FILL_KATHOLIKO_10      : PROC;
        DCL I      PIC  '99'  INIT (0);

        ACC10KATH  =(10)' ';   /*  йахокийо 10ЬГЖИО   */
   /*   PUT SKIP LIST('WS_PIC9_KATH_B = ',WS_PIC9_KATH_B); */
        DO I = 1 TO ORIO ;
            IF WS_PIC9_KATH_B = SUBSTR(P1KATHOL(I),1,6)
               THEN
               ACC10KATH = SUBSTR(P1KATHOL(I),10,10);

        END;

     END  FILL_KATHOLIKO_10     ;
  /**************************************************************
  *  FILL_KATHOLIKO_10               END                        *
  **************************************************************/
  /**************************************************************
  *  FILL_CIDB_NUMBER         START                             *
  *                                                             *
  **************************************************************/
     FILL_CIDB_NUMBER       : PROC;
       DCL MYEFARM CHAR(2);
       WPELATH = 0;                                                        05280

       WS_CUSTOMER_FOUND   = 0;

       T04CNV97.COD_PELATH = 0;

       /* CREATE ACCOUNT NUMBER FOR T04CNV97 */
       IF WS_IS_TAM = 1 THEN DO ;/*EINAI TAMIEYTHRIO*/
           MYEFARM = '90' ;
           WCIDB    =  '0'!!WS_PIC9_ARACC!!(7)' '!!'01';
       END;
       ELSE DO ;                   /*еимаи йатахесеис */
           MYEFARM = '70' ;
           WCIDB    =  WS_PIC9_ARACC!!(8)' '!!'01';
       END;


       EXEC SQL
            SELECT  COD_PELATH                                             05270
            INTO   :T04CNV97.COD_PELATH
            FROM   T04CNV97                                                05290
            WHERE  T04CNV97.COD_EFARM      = :MYEFARM AND                  05300
                   T04CNV97.NUM_AR_LOGAR   = :WCIDB                        05310
            WITH UR;                                                       05320
                                                                           05330
       IF SQLCODE ^= 0   THEN  DO;
            WS_CUSTOMER_FOUND = 1;
            CNT_REC_REJECT_T04CNV97 = CNT_REC_REJECT_T04CNV97 +1;
            ERLIN2 = 'пяобкгла **** еуяесг пекатг стом т04CNV97 : '     !!
                   !!' Aяихл.коцая. '
                   !! WCIDB
                   !!'  SQLCODE  : '!!SQLCODE;

         IF IS_TEST  ^= 'YES' THEN
              WRITE FILE(PRT) FROM(ERLINE);

       END;

       WPEL       =  T04CNV97.COD_PELATH ;                              05270000

       ACCCIDBNUM = WPEL ;                                              05270000

     END  FILL_CIDB_NUMBER      ;
  /**************************************************************
  *  FILL_CIDB_NUMBER         END                               *
  **************************************************************/
  /**************************************************************
  *  LOAD_TBL                 START                             *
  **************************************************************/

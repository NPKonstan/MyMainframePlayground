         ERLINE =
            'йулаимо ' !!  ALMK_EPIT    !!
            ' SPRCODE ' !!  ALMSPRCODE   !!
            ' KATH    ' !!  ALMKATH      !!
            ' DESCR   ' !!  ALMDESCR      ;
        WRITE FILE(PRT) FROM(ERLINE);

         ERLINE =
            'ONOMA ' !!  ALMCUSNAME   !!
            ' CIDB ' !!  ALMCIDBNUM   !!

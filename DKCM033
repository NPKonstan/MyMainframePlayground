        WRITE FILE(PRINT) FROM(LINE1);
        CTLCHR1 = ' ';
        GRAMMH = 'CTDKCM02.CHR_REJ_DESCR='!!CTDKCM02.CHR_REJ_DESCR;
        WRITE FILE(PRINT) FROM(LINE1);
        */
        EXEC SQL
            INSERT INTO CTDKCM02
                (
                 COD_CHANNEL,
                 KEY_AR_PAGIA,
                 COD_TYP_PAGIA,
                 COD_KATMA,
                 KEY_D_ARLOG1,
                 COD_D_EFARM1,
                 AMT_D_POSO,
                 DAT_REJECT,
                 COD_DEBCRE,
                 CHR_REJ_DESCR,
                 CHR_USER_ID,
                 KEY_TIMESTAMP,
                 KEY_TD_ARLOG1,
                 KEY_TD_ARLOG2
                )
            VALUES
                (
                 :CTDKCM02.COD_CHANNEL,
                 :CTDKCM02.KEY_AR_PAGIA,
                 :CTDKCM02.COD_TYP_PAGIA,
                 :CTDKCM02.COD_KATMA,
                 :CTDKCM02.KEY_D_ARLOG1,
                 :CTDKCM02.COD_D_EFARM1,
                 :CTDKCM02.AMT_D_POSO,

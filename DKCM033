           WHEN ('04')
               CTDKCM02.CHR_REJ_DESCR = 'вяеыстийо посо (т/й)';
               T60R2TRX.TRANSREJECTRSNCD = 'вяеыстийо посо (т/й)';
           WHEN ('05')

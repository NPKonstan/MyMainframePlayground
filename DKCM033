           WHEN ('97')
            CTDKCM02.CHR_REJ_DESCR = 'дивыс емдиалесг амамеысг(п/й)';
            T60R2TRX.TRANSREJECTRSNCD = 'дивыс емдиалесг амамеысг(п/й)';
           WHEN ('98')
            CTDKCM02.CHR_REJ_DESCR = 'посо памы апо ояио (п/й)';
            T60R2TRX.TRANSREJECTRSNCD = 'посо памы апо ояио (п/й)';
           WHEN ('99')

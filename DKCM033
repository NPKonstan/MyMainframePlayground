           WHEN ('06')
               CTDKCM02.CHR_REJ_DESCR = 'посо лгдем (т/й)';
               T60R2TRX.TRANSREJECTRSNCD = 'посо лгдем (т/й)';
           WHEN ('07')
               CTDKCM02.CHR_REJ_DESCR = 'коцаяиаслос йкеистос (т/й)';
               T60R2TRX.TRANSREJECTRSNCD = 'коцаяиаслос йкеистос (т/й)';
           WHEN ('08','10')
               CTDKCM02.CHR_REJ_DESCR = 'лг аподейто йахокийо (т/й)';
               T60R2TRX.TRANSREJECTRSNCD = 'лг аподейто йахокийо (т/й)';
           WHEN ('09')
             CTDKCM02.CHR_REJ_DESCR = 'амупаяйтос йыд. пекатг (т/й)';
             T60R2TRX.TRANSREJECTRSNCD = 'амупаяйтос йыд. пекатг (т/й)';
           WHEN ('11')
                  CTDKCM02.CHR_REJ_DESCR = 'посо ле дейадийа (т/й)';
                  T60R2TRX.TRANSREJECTRSNCD = 'посо ле дейадийа (т/й)';
           WHEN ('12')
              CTDKCM02.CHR_REJ_DESCR = 'посо памы апо то ояио (т/й)';

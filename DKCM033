                 :CTDKCM02.COD_DEBCRE,
                 :CTDKCM02.CHR_REJ_DESCR,
                 :CTDKCM02.CHR_USER_ID,
                 CURRENT TIMESTAMP,
                 :CTDKCM02.KEY_TD_ARLOG1,
                 :CTDKCM02.KEY_TD_ARLOG2
                );

            IF SQLCODE ^= 0 THEN DO;
               CTLCHR1 = ' ';
               GRAMMH = 'ERROR INSERTING INTO CTDKCM02, SQLCODE='!!
                        SQLCODE;
               WRITE FILE(PRINT) FROM(LINE1);
               CALL ERROR_ABEND;
            END;
            ELSE  INSCM02_CNT = INSCM02_CNT + 1;

 END WRITE_REJ;

 /*******************************************************************/
 INSERT_T60R2TRX:PROC;
 /*******************************************************************/

        T60R2TRX.SOURCESYSTEMID     =
        T60R2TRX.TRANSID            =
        T60R2TRX.TRANSDT            =
        T60R2TRX.TRANSCLEARDT       =
        T60R2TRX.TRANSTYPECD        =
        T60R2TRX.TRANSDIRECTIONCD   =
        T60R2TRX.TRANSAMT           =
        T60R2TRX.TRANSORGNALCRNCYCD =
        T60R2TRX.TRANSSTATUSCD      =
        T60R2TRX.TRANSREGECTRSNCD   =
        T60R2TRX.TRANSORIGINATORCD  =
        T60R2TRX.CONTRACTID         =
        T60R2TRX.TRANSCHANNELID     =
        T60R2TRX.TRANSCLEARNETWKCD  =
        T60R2TRX.TRANSCATEGORY      =

        EXEC SQL
            INSERT INTO T60R2TRX
                (
                 EFFDT,
                 SOURCESYSTEMID,
                 TRANSID,
                 TRANSDT,
                 TRANSTS,
                 TRANSCLEARDT,
                 TRANSTYPECD,
                 TRANSDIRECTIONCD,
                 TRANSAMT,
                 TRANSORGNALCRNCYCD,
                 TRANSSTATUSCD,
                 TRANSREGECTRSNCD,
                 TRANSORIGINATORCD,
                 CONTRACTID
                 TRANSCHANNELID,
                 TRANSCLEARNETWKCD,
                 TRANSCATEGORY
                )
            VALUES
                (
                 CURRENT DATE,
                 :T60R2TRX.EFFDT,
                 :T60R2TRX.SOURCESYSTEMID,
                 :T60R2TRX.TRANSID,
                 :T60R2TRX.TRANSDT,
                 CURRENT TIMESTAMP,
                 :T60R2TRX.TRANSCLEARDT,
                 :T60R2TRX.TRANSTYPECD,
                 :T60R2TRX.TRANSDIRECTIONCD,
                 :T60R2TRX.TRANSAMT,
                 :T60R2TRX.TRANSORGNALCRNCYCD,
                 :T60R2TRX.TRANSSTATUSCD,
                 :T60R2TRX.TRANSREGECTRSNCD,
                 :T60R2TRX.TRANSORIGINATORCD,
                 :T60R2TRX.CONTRACTID
                 :T60R2TRX.TRANSCHANNELID,
                 :T60R2TRX.TRANSCLEARNETWKCD,
                 :T60R2TRX.TRANSCATEGORY
                );
            IF SQLCODE ^= 0 THEN DO;
               CTLCHR1 = ' ';
               GRAMMH = 'ERROR INSERTING INTO T60R2TRX, SQLCODE='!!
                        SQLCODE;
               WRITE FILE(PRINT) FROM(LINE1);
            END;

 END INSERT_T60R2TR;

 /*=================================================*/
 ERROR_ABEND: PROC;
 /*=================================================*/
 DCL BOOM     DEC FIXED(1,0) INIT(0)  ;
 DCL ZERO     DEC FIXED(1,0) INIT(0)  ;

       CTLCHR1 = ' ';
       GRAMMH = 'TO пяOцяалла ха KAмеи CANCEL CANCEL CANCEL';
       WRITE FILE(PRINT) FROM(LINE1);
       BOOM = BOOM / ZERO;

 END ERROR_ABEND;


 TELOS:
        CTLCHR1 = ' ';
        GRAMMH = '===============================================';
        WRITE FILE(PRINT) FROM(LINE1);
        CTLCHR1 = ' ';
        GRAMMH = ' ейтупысг сулжымиас тоу пяоцяаллатос *DKCM033* ';
        WRITE FILE(PRINT) FROM(LINE1);
        CTLCHR1 = ' ';
        GRAMMH = '===============================================';
        WRITE FILE(PRINT) FROM(LINE1);
        /*
        CTLCHR1 = ' ';
        GRAMMH = ' DATETIME= ' !! DATETIME;
        WRITE FILE(PRINT) FROM(LINE1);
        */
        CTLCHR1 = ' ';
        GRAMMH = ' глеяолгмиа KAI ыяа яогс : ' !!
               SYSDD!!'.'!!SYSMM!!'.'!!SYSY1!!SYSY2 !!'-'!!
               SYSHH!!':'!!SYSMI!!':'!!SYSSS!!
                 '  еяцасиас : '!! CURR_DATE;
        WRITE FILE(PRINT) FROM(LINE1);

        CTLCHR1 = ' ';
        GRAMMH = '-------------------------------------------------'!!
                 '-------------------------';
        WRITE FILE(PRINT) FROM(LINE1);

        CTLCHR1 = ' ';
        GRAMMH = 'апамтгсеис йыдийым 4528 йаи 4529                  ='!!
        ANSWR_CNT;
        WRITE FILE(PRINT) FROM(LINE1);

        CTLCHR1 = ' ';
        GRAMMH = '-------------------------------------------------'!!
                 '-------------------------';
        WRITE FILE(PRINT) FROM(LINE1);
        CTLCHR1 = ' ';
        GRAMMH = 'епитувеис апамтгсеис йыдийым 4528 йаи 4529        ='!!
        ANSWR_S_CNT;
        WRITE FILE(PRINT) FROM(LINE1);
        CTLCHR1 = ' ';
        GRAMMH = 'амепитувеис апамтгсеис йыдийым 4528 йаи 4529      ='!!
        ANSWR_U_CNT;
        WRITE FILE(PRINT) FROM(LINE1);
        CTLCHR1 = ' ';
        GRAMMH = 'еццяажес стом пIмайа апояяиьеым (CTDKCM02)        ='!!
        INSCM02_CNT;
